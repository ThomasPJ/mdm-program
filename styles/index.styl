body
  background-color: #eee
  font-family: sans-serif

h1
  margin: 0
  color: #000

h2
  margin: 0
  font-weight: normal
  color: #969696
  font-style: italic
  font-size: 17px

ul
  margin: 0
  padding: 0
  li
    list-style-type: none
    margin: 20px 0
    a
      text-decoration: underline
      color: #2DAADE
      font-weight: bold
    a:hover
      text-decoration: none
    p
      margin: 0
